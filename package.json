    "styled-components": "^1.4.5",
    "superagent": "^3.5.2",
    "ts-node": "^3.0.2",
    "typescript": "^2.2.2",
    "uuid": "^3.0.1",
    "webfontloader": "^1.6.27",
    "yargs": "^7.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.3",
    "@types/chalk": "^0.4.31",
    "@types/classnames": "0.0.32",
    "@types/color": "^1.0.1",
    "@types/compression": "0.0.33",
    "@types/deep-freeze": "0.0.29",
