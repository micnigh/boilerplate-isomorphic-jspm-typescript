{
  "name": "boilerplate-isomorphic-typescript",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "postinstall": "cross-env node ./npm-scripts/postinstall.js",
    "postshrinkwrap": "cross-env node ./npm-scripts/postshrinkwrap.js",
    "build": "cross-env FORCE_COLOR=1 npm-run-all build:lib build:bundle",
    "build:bundle": "cross-env webpack",
    "build:lib": "cross-env ts-node --fast tasks/lib.ts --exec",
    "server": "cross-env ts-node --fast --project server server/index.ts",
    "server:dev": "cross-env FORCE_COLOR=1 npm-run-all --parallel server:dev:**",
    "server:dev:db": "cross-env docker-compose up",
    "server:dev:web": "cross-env nodemon --watch server --ext js,jsx,ts,tsx,hbs,json --exec npm run server",
    "test": "cross-env FORCE_COLOR=1 npm-run-all --parallel test:jest test:browser",
    "test:jest": "cross-env jest",
    "test:browser": "cross-env TS_NODE_FAST=true karma start karma.config.js --single-run --colors",
    "test:watch": "cross-env FORCE_COLOR=1 npm-run-all --parallel test:watch:**",
    "test:watch:jest": "cross-env jest --watchAll",
    "test:watch:karma": "cross-env TS_NODE_FAST=true karma start karma.config.js --colors",
    "test:coverage": "cross-env jest --coverage"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.1",
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "color": "^1.0.3",
    "compression": "^1.6.2",
    "connect-session-sequelize": "^4.1.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^4.0.0",
    "css-wipe": "^4.3.0",
    "denormalizr": "^0.5.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "grid-styled": "^1.0.1",
    "handlebars": "^4.0.6",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "mime": "^1.3.4",
    "mkdirp": "^0.5.1",
    "moment": "^2.18.1",
    "morgan": "^1.8.1",
    "mysql": "^2.13.0",
    "normalizr": "^3.2.2",
    "passport": "^0.3.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
