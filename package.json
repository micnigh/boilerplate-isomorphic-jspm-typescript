  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.node\\.spec\\.tsx?$",
    "roots": [
      "<rootDir>/client/",
      "<rootDir>/server/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/\\.*/",
      "\\.browser\\.spec\\.tsx?$"
    ],
    "globals": {
      "__TEST__": true,
      "__TS_CONFIG__": {
        "target": "es5",
        "module": "commonjs",
        "jsx": "react"
      }
    }
  }
}
